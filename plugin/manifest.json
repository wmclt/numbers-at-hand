{
  "manifest_version": 2,

  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "homepage_url": "https://github.com/wmclt/numbers-at-hand",
  "version": "1.14",
  "default_locale": "en",

  "browser_action": {
    "default_icon": {
      "16": "icons/Icon-16.png",
      "32": "icons/Icon-32.png",
      "48": "icons/Icon-48.png",
      "128": "icons/Icon-128.png"
    },
    "default_popup": "popup.html",
    "default_title": "__MSG_extensionHoverText__"
  },
  "permissions": ["storage", "contextMenus", "activeTab"],
  "background": {
    "scripts": ["js/common.js", "js/background.js"]
  },
  "icons": {
    "16": "icons/Icon-16.png",
    "32": "icons/Icon-32.png",
    "48": "icons/Icon-48.png",
    "128": "icons/Icon-128.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "numbersathand@example.org",
      "strict_min_version": "57.0"
    }
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://www.google-analytics.com https://www.googletagmanager.com ; object-src 'self'"
}